<%= form_for [account, product], {multipart: true} do |f| %>
  <div class="form-group">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :description %><br />
    <%= f.text_area :description, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :item_number %><br />
    <%= f.text_field :item_number, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :style_number %><br />
    <%= f.text_field :style_number, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :vendor %><br />
    <%= f.text_field :vendor, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :product_page_url %><br />
    <%= f.text_field :product_page_url, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit "Create", class: 'btn btn-outline-secondary' %>
  </div>

<% end %>
